{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": "off",
    "react/react-in-jsx-scope": "off",
    "jsx-quotes": [2, "prefer-single"],
    "arrow-parens": "off",
    "no-unused-vars": "warn",
    "react/prop-types": "off"
  }
}
